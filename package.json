{
  "name": "perf-cascade",
  "version": "0.2.18",
  "description": "Har file visualizer",
  "main": "./index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "grunt tslint",
    "watch": "npm run start & grunt default",
