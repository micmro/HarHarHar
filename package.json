    "lib": "./lib",
    "dist": "./dist"
  },
  "types": "./index.d.ts",
  "license": "MIT",
  "devDependencies": {
    "grunt": "^1.0.1",
    "grunt-banner": "^0.6.0",
    "grunt-browserify": "^5.0.0",
    "grunt-bump": "^0.8.0",
    "grunt-contrib-clean": "^1.0.0",
    "grunt-contrib-concat": "^1.0.1",
    "grunt-contrib-copy": "^1.0.0",
    "grunt-contrib-uglify": "^2.0.0",
    "grunt-contrib-watch": "^1.0.0",
    "grunt-gh-pages": "^2.0.0",
    "grunt-run": "^0.6.0",
    "grunt-tslint": "^4.0.0",
    "live-server": "^1.1.0",
    "load-grunt-tasks": "^3.5.2",
    "tsify": "^2.0.2",
    "tslint": "^4.2.0"
  },
  "dependencies": {
    "tsify": "^2.0.3",
    "typescript": "^2.1.4"
  }
